local Tab1 = Window:CreateTab("Home", "app-window")
local PlayerSection = Tab1:CreateSection("Local Player")
local InfiniteJumpEnabled = false

Tab1:CreateToggle({
    Name = "Infinite Jump",
    CurrentValue = false,
    Flag = "ToggleInfiniteJump",
    Callback = function(Value)
        InfiniteJumpEnabled = Value
    end,
})

-- Script cho infinite jump
game:GetService("UserInputService").JumpRequest:Connect(function()
    if InfiniteJumpEnabled then
        local Player = game:GetService("Players").LocalPlayer
        if Player and Player.Character and Player.Character:FindFirstChild("Humanoid") then
            Player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end
end)

local SpeedValue = 16 -- Tá»‘c Ä‘á»™ máº·c Ä‘á»‹nh cá»§a nhÃ¢n váº­t
local Slider

-- Khá»Ÿi táº¡o Slider sau khi Ä‘áº£m báº£o Tab1 Ä‘Ã£ Ä‘Æ°á»£c táº¡o
Slider = Tab1:CreateSlider({
    Name = "Walk Speed",
    Range = {16, 300}, -- Tá»‘c Ä‘á»™ tá»‘i thiá»ƒu vÃ  tá»‘i Ä‘a
    Increment = 1, -- BÆ°á»›c tÄƒng
    Suffix = "Speed", -- ÄÆ¡n vá»‹ hiá»ƒn thá»‹
    CurrentValue = 16, -- GiÃ¡ trá»‹ ban Ä‘áº§u
    Flag = "WalkSpeedSlider",
    Callback = function(Value)
        SpeedValue = Value -- Cáº­p nháº­t giÃ¡ trá»‹ tá»‘c Ä‘á»™
        local Player = game:GetService("Players").LocalPlayer
        if Player and Player.Character and Player.Character:FindFirstChild("Humanoid") then
            Player.Character.Humanoid.WalkSpeed = SpeedValue
        end
    end,
})

-- Äáº£m báº£o tá»‘c Ä‘á»™ Ä‘Æ°á»£c duy trÃ¬
game:GetService("RunService").Stepped:Connect(function()
    local Player = game:GetService("Players").LocalPlayer
    if Player and Player.Character and Player.Character:FindFirstChild("Humanoid") then
        Player.Character.Humanoid.WalkSpeed = SpeedValue
    end
end)

-- Táº¡o nÃºt Ä‘á»ƒ reset slider
Tab1:CreateButton({
    Name = "Reset Walk Speed",
    Callback = function()
        SpeedValue = 16 -- Äáº·t láº¡i giÃ¡ trá»‹ tá»‘c Ä‘á»™
        if Slider then
            Slider:Set(16) -- Cáº­p nháº­t slider vá» giÃ¡ trá»‹ máº·c Ä‘á»‹nh
        end
        local Player = game:GetService("Players").LocalPlayer
        if Player and Player.Character and Player.Character:FindFirstChild("Humanoid") then
            Player.Character.Humanoid.WalkSpeed = SpeedValue
        end
    end,
})
