local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "13:30",
    Icon = "", -- Cung cáº¥p ID hÃ¬nh áº£nh há»£p lá»‡ hoáº·c Ä‘á»ƒ trá»‘ng
    LoadingTitle = "Why I can't...",
    LoadingSubtitle = "by Neko",
    Theme = "Default",
    DisableRayfieldPrompts = false,

    DisableBuildWarnings = false,
    ConfigurationSaving = {
        Enabled = false,
        FolderName = "MTF", -- Äáº·t tÃªn folder dÆ°á»›i dáº¡ng chuá»—i
        FileName = "Uni"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = true, 
    KeySettings = {
        Title = "Nháº­p key Ä‘á»ƒ tiáº¿p tá»¥c ğŸ‘€",
        Subtitle = "há»‡ thá»‘ng key",
        Note = "Ä‘Ã©o cÃ³ cÃ¡ch Ä‘á»ƒ láº¥y key Ä‘Ã¢u",
